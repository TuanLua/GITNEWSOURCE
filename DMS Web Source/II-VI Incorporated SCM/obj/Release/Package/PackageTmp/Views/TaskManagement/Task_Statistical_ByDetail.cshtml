@section scripts {

    <script type="text/javascript">
        $(document).ready(function () {
            var id = '@User.Identity.GetUserId()';
            $.blockUI();
            $.post('@Url.Action("Getnamebyid", "WriteNCRForIQC")', { id: id },
                function (result) {
               $("#name").val(result);
